{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start:es": "ng serve --aot --configuration=es",
    "start:hi": "ng serve --aot --configuration=hi",
    "start:pt": "ng serve --aot --configuration=pt",
    "copy-web-inf": "cp -R ../backend/src/main/WEB-INF ../backend/src/main/webapp",
    "build": "ng build --prod --localize && npm run copy-web-inf",
    "build:ci": "ng build --prod",
    "build:dev": "ng build --configuration=developer && npm run copy-web-inf",
    "build:es": "ng build --configuration=production-es && npm run copy-web-inf",
    "build:hi": "ng build --configuration=production-hi && npm run copy-web-inf",
    "build:pt": "ng build --configuration=production-pt && npm run copy-web-inf",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "view-code-coverage": "http-server ./coverage/frontend"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~9.1.11",
    "@angular/google-maps": "^10.1.0",
    "@angular/localize": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@types/jest": "^26.0.8",
    "http-server": "^0.12.3",
    "lodash": "^4.17.15",
    "mat-table-exporter": "^9.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "xlf-translator": "^1.0.30",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@types/googlemaps": "^3.39.11",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "puppeteer": "^5.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
